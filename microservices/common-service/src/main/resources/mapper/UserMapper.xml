<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.sunyesle.microservices.common.user.UserMapper">
    <select id="selectById" parameterType="map" resultType="com.sunyesle.microservices.common.user.User">
        SELECT id, name, phone_number, email, created_at, updated_at
        FROM tb_user
        WHERE id = #{id}
    </select>
</mapper>
